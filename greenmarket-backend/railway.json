{
  "project": {
    "name": "umc-green"
  },
  "services": [
    {
      "name": "backend",
      "path": "greenmarket-backend",
      "start": "cd greenmarket-backend && node server.js",
      "build": "cd greenmarket-backend && npm ci",
      "env": {
        "NODE_ENV": "production"
      }
    }
  ],
  "plugins": [
    {
      "name": "mysql",
      "service": "backend",
      "options": {
        "database": "greenmarket"
      }
    }
  ]
}
