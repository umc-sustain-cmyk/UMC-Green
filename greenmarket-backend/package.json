{
  "name": "greenmarket-backend",
  "version": "1.0.0",
  "description": "Backend API for GreenMarket - campus donation and item-sharing platform",
  "main": "server.js",
  "keywords": [
    "node",
    "express",
    "mysql",
    "sequelize",
    "authentication",
    "api"
  ],
  "author": "UMC Green Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "cross-env": "^7.0.3",
    "sqlite3": "^5.1.6",
    "sequelize-cli": "^6.6.0"
  },
  "engines": {
    "node": "18.x"
  },
  "sequelize": {
    "config": "./config/config.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all"
    ,
    "seed": "npx sequelize-cli db:seed:all"
  }
}